extends ../layout

block content
   div.page-header
      h1 Users

   div.row
      div.col-md-9
         div.list-group
            - for (var i=0; i < users.length; i++) {
               a.list-group-item(href='/admin/users/' + users[i]._id) #{users[i].name}&nbsp;
                  small ( #{users[i].username} )
            - }
      
      div.col-md-3
         include nav